<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="volley.volleyball"
    android:installLocation="auto">

    <uses-sdk android:minSdkVersion="15"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/volleyball_ball"
        android:label="Volleyball log"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GamesListActivity"
            android:label="Volleyball log">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeamsActivity"
            android:label="Teams and players" >
        </activity>
        <activity
            android:name=".ServeActivity"
            android:label="Actions" >
        </activity>
        <activity
            android:name=".PlayerServeStatsActivity"
            android:label="Statistics" >
        </activity>
    </application>

</manifest>
